<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>BeetleX</RootNamespace>
    <Authors>henryfan</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <AssemblyVersion>1.4.9.6</AssemblyVersion>
    <FileVersion>1.4.9.6</FileVersion>
    <Version>1.4.9.6</Version>
    <Description>high performance dotnet core socket tcp communication components,  support tcp, ssl, http, websocket, rpc, redis ... protocols and 1M connections problem solution(dotnet standard 2.0)</Description>
    <Copyright>Copyright © beetlex.io 2019-2020 email:henryfan@msn.com</Copyright>
    <PackageProjectUrl>https://github.com/IKende/BeetleX</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/IKende/FastHttpApi/blob/master/LICENSE</PackageLicenseUrl>
    <Company>beetlex.io</Company>
    <AutoGenerateBindingRedirects>True</AutoGenerateBindingRedirects>
    <AssemblyName>BeetleX</AssemblyName>
    <LangVersion>7.3</LangVersion>
    <PackageId>BeetleX</PackageId>
    <PackageReleaseNotes>high performance dotnet core socket tcp communication components,  support tcp, ssl, http, websocket, rpc, redis ... protocols and 1M connections problem solution(dotnet standard 2.0)</PackageReleaseNotes>
    <SignAssembly>false</SignAssembly>
    <AssemblyOriginatorKeyFile>beetlex.pfx</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <DefineConstants>TRACE;STD2</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <DefineConstants>TRACE;STD2</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\..\src\BeetleX\Buffers\PipeStreamExtens.cs" Link="Buffers\PipeStreamExtens.cs" />
    <Compile Include="..\..\src\BeetleX\Clients\AwaiterClient.cs" Link="Clients\AwaiterClient.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Buffers" Version="4.5.0" />
    <PackageReference Include="System.Memory" Version="4.5.2" />
  </ItemGroup>

</Project>
